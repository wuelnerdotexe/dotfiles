{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"osc99": true,
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"type": "text",
					"style": "plain",
					"foreground": "#787C99",
					"properties": {
						"prefix": "\u250c ",
						"text": "Insert Command"
					}
				},
				{
					"type": "path",
					"style": "powerline",
					"powerline_symbol": "\ue0b0",
					"foreground": "#16161E",
					"background": "#7DCFFF",
					"properties": {
						"style": "full",
						"enable_hyperlink": false
					}
				},
				{
					"type": "git",
					"style": "powerline",
					"powerline_symbol": "\ue0b0",
					"foreground": "#16161E",
					"background": "#BB9AF7",
					"properties": {
						"branch_ahead_icon": "\u2191",
						"branch_behind_icon": "\u2193",
						"branch_gone": "\u2262",
						"branch_icon": "\ue0a0 ",
						"branch_identical_icon": "\u2261",
						"cherry_pick_icon": "\u2713 ",
						"color_background": true,
						"commit_icon": "\u25b7 ",
						"display_status_detail": true,
						"local_staged_icon": "",
						"local_working_icon": "",
						"merge_icon": "\u25f4 ",
						"no_commits_icon": "[no commits]",
						"rebase_icon": "\u2c62 ",
						"stash_count_icon": "",
						"status_colors_enabled": true,
						"status_separator_icon": " \u2502",
						"tag_icon": "\u25b6 ",
						"local_changes_color": "#E0AF68",
						"ahead_and_behind_color": "#ff9e64",
						"behind_color": "#ff9e64",
						"ahead_color": "#41A6B5"
					}
				},
				{
					"type": "terraform",
					"style": "powerline",
					"powerline_symbol": "\ue0b0",
					"foreground": "#16161E",
					"background": "#BB9AF7"
				},
				{
					"type": "exit",
					"style": "powerline",
					"powerline_symbol": "\ue0b0",
					"foreground": "#16161E",
					"background": "#F7768E",
					"properties": {
						"display_exit_code": false,
						"prefix": " ",
						"postfix": " ",
						"error_icon": "error"
					}
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"type": "text",
					"style": "plain",
					"foreground": "#787C99",
					"properties": {
						"prefix": "",
						"postfix": "",
						"text": "\u2514 "
					}
				},
				{
					"type": "text",
					"style": "plain",
					"foreground": "#787C99",
					"properties": {
						"prefix": "",
						"postfix": "",
						"text": "$"
					}
				}
			]
		}
	],
	"final_space": true
}
